    display: flex;
    justify-content: center;
}

.hover1 {
    opacity: 0.7;
    background-color: rgba(0, 0, 0, 0.1)
}

.lineclamp1 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.lineclamp2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-break: break-all;
}

.tab_bar_border {
    position: fixed;
    z-index: 999999;
    bottom: 0;
    left: 0;
    width: 100%;
    border-bottom: 1rpx solid #e2e2e2;
}


/** 表单 **/

.wx-cell {
    height: 89rpx;
    line-height: 89rpx;
    position: relative;
    font-size: 28rpx;
    color: #333333;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.wx-cell-2 {
    position: relative;
    font-size: 28rpx;
    color: #333333;
}

.wx-cell-3 {
    position: relative;
    font-size: 28rpx;
    color: #333333;
}

.wx-cell:after {
    position: absolute;
    content: '';
    width: 96%;
    left: 31rpx;
    bottom: 0;
    height: 1px;
    background-color: #e3e5e9;
    transform: scale(1, 0.5);
    transform-origin: center bottom;
}

.wx-cell-2:after {
    position: absolute;
    content: '';
    width: 96%;
    left: 31rpx;
    bottom: 0;
    height: 1px;
    background-color: #e3e5e9;
    transform: scale(1, 0.5);
    transform-origin: center bottom;
}

.wx-cell .wx-label {
    display: block;
    word-wrap: break-word;
    word-break: break-all;
    white-space: nowrap;
    min-width: 140rpx;
}

.wx-cell-bd {
    flex: 1;
}

.wx-input {
    width: 100%;
    border: 0;
    outline: 0;
    -webkit-appearance: none;
    background-color: transparent;
    font-size: inherit;
    color: inherit;
}

.wx-allow:after {
    position: absolute;
    top: 50%;
    width: 20rpx;
    height: 20rpx;
    display: inline-block;
    right: 30rpx;
    border: solid #c7c7cc;
    border-width: 4rpx 4rpx 0 0;
    transform: translate(0, -50%) rotate(45deg);
    content: ""
}

.wx-textarea {
    display: block;
    border: 0;
    resize: none;
    width: 90%;
    color: inherit;
    outline: 0;
    height: 161rpx;
    margin: 28rpx 0 0 30rpx;
}

.wx-placeholder-css {
    font-size: 28rpx;
    color: #999999;
}

.wx-save {
    background: #E02020;
    font-size: 32rpx;
    color: #fff;
    width: 92%;
    height: 89rpx;
    line-height: 89rpx;
    margin-top: 59rpx;
}

button {
    border-radius: 5rpx;
}

.wx-wq-label {
    padding: 30rpx 0 0 30rpx;
}

.wx-process {
    position: absolute;
    top: 24rpx;
    left: 49rpx;
    bottom: 0;
    width: 4rpx;
    background: #ddd;
}

.wx-search-clear {
    width: 30rpx;
    height: 30rpx;
}

.wx-layout {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    width: 100%;
    height: 100%;
}


/* 清楚浮动 */

.clearfix:before, .clearfix:after {
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    zoom: 1;
}


/* IE < 8 */


/**
loading 效果 
**/

.loading {
    display: flex;
    margin: 30rpx auto;
    font-size: 10px;
    position: relative;
    text-indent: -9999em;
    border-top: 3rpx solid #999999;
    border-right: 3rpx solid #999999;
    border-bottom: 3rpx solid #999999;
    border-left: 3rpx solid #ffffff;
    transform: translateZ(0);
    animation: loading 1.1s infinite linear;
}

.loading, .loading:after {
    border-radius: 50%;
    width: 50rpx;
    height: 50rpx;
}

@keyframes loading {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(360deg);
    }
}


/*
底部弹框
**/

.mode {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 590rpx;
    background: #fff;
    z-index: 1201;
    transition: transform .2s cubic-bezier(0, 0, .25, 1);
    transform: translate3d(0, 100%, 0);
}

.mode .mode-title {
    width: 100%;
    height: 102rpx;
    line-height: 102rpx;
    text-align: center;
    font-size: 30rpx;
    color: #333333;
    position: relative;
}

.mode .mode-title .mode-txt {
    display: inline-block;
}

.mode .mode-title .close {
    display: inline-block;
    width: 100rpx;
    height: 60rpx;
}

.mode .mode-title .close image {
    width: 60rpx;
    height: 100%;
}

.mode .mode-line:after {
    position: absolute;
    content: '';
    width: 100%;
    left: 0;
    bottom: 0;
    height: 1px;
    background-color: #e3e5e9;
    transform: scale(1, 0.5);
    transform-origin: center bottom
}

.mode .mode-list {
    height: 102rpx;
    line-height: 102rpx;
    padding: 0 34rpx 0 31rpx;
    position: relative;
    align-items: center;
}

.mode .mode-icon {
    vertical-align: -8rpx;
}

.mode .mode-list .label-bottom .txt {
    font-size: 28rpx;
    color: #333333;
}

.mode .mode-list .weixin {
    width: 48rpx;
    height: 38rpx;
}

.mode .mode-list .daohuo {
    width: 42rpx;
    height: 46rpx;
}

.mode .mode-list .t1 {
    vertical-align: 10rpx;
    padding-left: 21rpx;
}

button::after {
    border: 0
}

