/** CSS3 **/

.flex {
    display: flex;
}

.flex_1 {
    flex: 1;
}

.flex-pack-j {
    display: flex;
    justify-content: space-between;
}

.flex-align-center {
    display: flex;
    align-items: center;
}

.flex-align-start {
    display: flex;
    align-items: flex-start;
}

.flex-wrap {
    display: flex;
