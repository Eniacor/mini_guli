.header {
  width: 100%;
  height: 200rpx;
  background: #f9f9f9;
  overflow: hidden;
}
.icong {
  width: 64rpx;
  height: 56rpx;
  margin-left: 50rpx;
  margin-top: 66rpx;
  float: left;
}
.icong image {
  width: 64rpx;
  height: 56rpx;
}
.border-shu {
  float: left;
  margin-top: 76rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  height: 58rpx;
  border-left: #666 solid 1rpx;
}
.header-test {
  float: left;
  overflow: hidden;
}
.words-a {
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 60rpx;
  line-height: 1;
}
.words-b {
  font-size: 26rpx;
  line-height: 60rpx;
  color: #222;
}
.words {
  width: 750rpx;
  background: #fff;
}
.words-border-info {
  width: 690rpx;
  margin: auto;
}
.words-bt {
  float: left;
  margin-top: 40rpx;
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.words-bt-info {
  float: left;
  margin-top: 40rpx;
  margin-left: 20rpx;
  font-size: 30rpx;
  color: #666;
}
.images {
  float: left;
  width: 26rpx;
  height: 25rpx;
  margin-top: 35rpx;
  margin-left: 20rpx;
}
.img {
  width: 26rpx;
  height: 25rpx;
}
.explain {
  font-size: 28rpx;
  color: #666;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
  line-height: 50rpx;
  clear: both;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.border-noe {
  width: 690rpx;
  border-bottom: 1rpx solid #e0e0e0;
  margin: 30rpx 0 0 0;
}
.words-border-info:last-child .border-noe {
  border-bottom: 1rpx solid #fff;
}
