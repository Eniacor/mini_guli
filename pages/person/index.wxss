page{
    background: #f2f2f2;
}
.header{
    width:750rpx;
    height: 200rpx;
    background: #fff;
    overflow: hidden;
}
.user-image-tou{
    width: 100rpx;
    height: 100rpx;
    margin-top: 50rpx;
    background: green;
    margin-left: 40rpx;
    border-radius: 100rpx;
    float: left;
}
.user-border{
    width: 500rpx;
    height: 140rpx;
    font-size: 38rpx;
    float: left;
    line-height: 1;
    color: #333;
    margin-left: 30rpx;
    /* background: red; */
    overflow: hidden;
}
.user-name{
    margin-top: 80rpx;
}
.user-number{
    font-size: 30rpx;
    margin-top: 10rpx;
}
.section{
    width: 690rpx;
    height: 80rpx;
    background: #fff;
    padding: 12rpx 20rpx 12rpx 40rpx;
    margin-top: 5rpx;
    overflow: hidden;
    color: #333;
    font-size: 28rpx;
    line-height: 80rpx;
}
.section-title-user{
    float: left;
}
.section-title-users{
    float: right;
    line-height: 80rpx;
}
.section-title-ict{
    float: right;
    line-height: 76rpx;
}
.section-title-icts{
    float:left;
    line-height: 76rpx;
}
