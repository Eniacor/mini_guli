.top {
  width: 750rpx;
  height: 60rpx;
  background: #fff;
  padding-top: 28rpx;
}
.top .progress {
  width: 511rpx;
  height: 32rpx;
  margin: 0 auto;
  position: relative;
}
.top .progress .progress_back {
  position: absolute;
  width: 511rpx;
  height: 32rpx;
  background: #EDEAF1;
  z-index: 0;
  border-radius: 20rpx;
}
.top .progress .progress_color {
  position: absolute;
  width: 15%;
  height: 32rpx;
  background: #FFD944;
  z-index: 0;
  border-radius: 20rpx;
}
.top .progress .progress_txt {
  position: absolute;
  top: 9rpx;
  left: 42%;
  font-size: 16rpx;
  line-height: 16rpx;
  color: #333;
}
.avatar {
  margin-top: 16rpx;
}
.avatar .item {
  height: 136rpx;
  background: #fff;
  overflow: hidden;
}
.avatar .item .left_txt {
  float: left;
  margin-top: 53rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333333;
  margin-left: 35rpx;
}
.avatar .item .right_img1 {
  float: right;
  margin-top: 21rpx;
  width: 94rpx;
  height: 94rpx;
  border-radius: 10rpx;
  background: #EDEAF1;
}
.avatar .item .right_img {
  float: right;
  margin-top: 5rpx;
  width: 16rpx;
  height: 27rpx;
  vertical-align: bottom;
  margin-left: 42rpx;
  margin-right: 35rpx;
  margin-top: 55rpx;
}
.lists {
  margin-top: 16rpx;
}
.lists .item {
  padding: 32rpx 35rpx;
  background: #fff;
  overflow: hidden;
}
.lists .item .left_img {
  float: left;
  width: 36rpx;
  height: 36rpx;
  vertical-align: bottom;
}
.lists .item .left_txt {
  float: left;
  margin-top: 4rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333333;
}
.lists .item .right_img {
  float: right;
  margin-top: 5rpx;
  width: 16rpx;
  height: 27rpx;
  vertical-align: bottom;
}
