.collect .header {
  width: 100%;
  height: 200rpx;
  background: #f9f9f9;
  overflow: hidden;
}
.collect .icong {
  width: 64rpx;
  height: 56rpx;
  margin-left: 50rpx;
  margin-top: 66rpx;
  float: left;
}
.collect .icong image {
  width: 64rpx;
  height: 56rpx;
}
.collect .border-shu {
  float: left;
  margin-top: 76rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  height: 58rpx;
  border-left: #666 solid 1rpx;
}
.collect .header-test {
  float: left;
  overflow: hidden;
}
.collect .words-a {
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 60rpx;
  line-height: 1;
}
.collect .words-b {
  font-size: 26rpx;
  line-height: 60rpx;
  color: #222;
}
.collect .items .item {
  width: 610rpx;
  height: 170rpx;
  margin: 0 auto;
  margin: 30rpx 30rpx 0 30rpx;
  background: #fff;
  border-radius: 10rpx;
  padding: 40rpx;
}
.collect .items .item .item_title {
  height: 32rpx;
  line-height: 32rpx;
}
.collect .items .item .item_title .title_line {
  float: left;
  width: 12rpx;
  height: 32rpx;
  margin-right: 20rpx;
  border-radius: 8rpx;
  background: #d43c33;
}
.collect .items .item .item_title .title_txt {
  float: left;
  font-size: 32rpx;
  color: #d43c33;
  font-weight: 600;
}
.collect .items .item .item_content {
  margin-top: 46rpx;
  position: relative;
}
.collect .items .item .item_content .content_left {
  position: absolute;
  top: 6rpx;
  left: 0;
  width: 80rpx;
  height: 80rpx;
}
.collect .items .item .item_content .content_left image {
  width: 100%;
  height: 100%;
}
.collect .items .item .item_content .content_middle {
  position: absolute;
  top: 18rpx;
  left: 120rpx;
  width: 1rpx;
  height: 56rpx;
  background: #666;
}
.collect .items .item .item_content .content_right {
  position: absolute;
  top: 0;
  left: 161rpx;
}
.collect .items .item .item_content .content_right .right_title {
  display: inline-block;
  height: 46rpx;
  line-height: 46rpx;
  padding: 0 10rpx;
  color: #fff;
  text-align: center;
  border-radius: 8rpx;
  font-size: 26rpx;
}
.collect .items .item .item_content .content_right .backcolor_yellow {
  background: #face64;
}
.collect .items .item .item_content .content_right .backcolor_blue {
  background: #85afec;
}
.collect .items .item .item_content .content_right .backcolor_green {
  background: #70b8ac;
}
.collect .items .item .item_content .content_right .backcolor_purple {
  background: #a96dc9;
}
.collect .items .item .item_content .content_right .right_des {
  height: 26rpx;
  line-height: 26rpx;
  font-size: 26rpx;
  margin-top: 20rpx;
  color: #222;
}
.collect .score {
  background: #fff;
}
.collect .score .score_contents .score_content {
  width: 690rpx;
  margin: 0 30rpx;
  height: 112rpx;
  line-height: 112rpx;
  border-bottom: 1rpx solid #e0e0e0;
}
.collect .score .score_contents .score_content .content_left {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
}
.collect .score .score_contents .score_content .content_left image {
  width: 100%;
  height: 100%;
  vertical-align: middle;
}
.collect .score .score_contents .score_content .content_middle {
  display: inline-block;
  font-size: 30rpx;
  color: #222;
}
.collect .score .score_contents .score_content .content_right {
  float: right;
  font-size: 30rpx;
  color: #d43c33;
}
.collect .score .score_contents .score_content .content_right image {
  width: 12rpx;
  height: 22rpx;
  margin-left: 15rpx;
}
.collect .score .score_contents .score_content:last-child {
  border: 0;
}
