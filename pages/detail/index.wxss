.detail {
  background: #f6f6f6;
  padding-bottom: 140rpx;
}
.detail .detail_img {
  width: 750rpx;
  height: 400rpx;
  font-weight: 600;
}
.detail .detail_img .img {
  width: 100%;
  height: 100%;
}
.detail .detail_title {
  padding: 20rpx 30rpx;
  background: #fff;
}
.detail .detail_title .title {
  font-size: 30rpx;
  margin-bottom: 16rpx;
}
.detail .detail_title .start {
  font-size: 24rpx;
  color: #999;
}
.detail .detail_des {
  padding: 20rpx 30rpx;
  background: #f7e6d6;
  font-size: 24rpx;
  color: #999;
  margin: 15rpx 0;
}
.detail .background {
  background: #fff;
  padding: 30rpx 0;
}
.detail .background .detail_focus {
  margin: 30rpx;
  padding: 30rpx;
  border: 4rpx solid #d8d5ca;
}
.detail .background .detail_focus .focus_title {
  font-size: 24rpx;
  font-weight: 600;
  color: #666;
  width: 540rpx;
  height: 80rpx;
  margin: 0 auto;
  text-align: center;
}
.detail .background .detail_focus .focus_des {
  margin: 34rpx 0;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 22rpx;
  background: #f6f6f6;
  color: #999;
}
.detail .background .detail_focus .focus_zhi {
  width: 240rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 22rpx;
  background: #75726a;
  border-radius: 40rpx;
  color: #fff;
  margin: 0 auto;
}
.detail .detail_chou {
  width: 140rpx;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  border-radius: 50%;
  font-size: 22rpx;
  background: #e8c3c3;
  color: #ece7e7;
  margin: 0 auto;
  margin-top: 60rpx;
  margin-bottom: 30rpx;
}
.detail .detail_num {
  font-size: 24rpx;
  text-align: center;
  color: #999;
}
.detail .detail_num text {
  color: #045fbb;
}
.detail .detail_can {
  padding-left: 200rpx;
  padding-top: 20rpx;
}
.detail .detail_can .item {
  display: inline-block;
  width: 318rpx;
  width: 46rpx;
  height: 46rpx;
  background: #045fbb;
  margin-right: 6rpx;
}
.detail .detail_can .item:last-child {
  margin-right: 0;
}
.detail .detail_bottom {
  position: fixed;
  bottom: 0;
  background: #fff;
  width: 750rpx;
  height: 90rpx;
  padding-top: 20rpx;
}
.detail .detail_bottom .bottom_left {
  display: inline-block;
  width: 345rpx;
  height: 70rpx;
  background: #75726a;
  color: #fff;
  font-size: 24rpx;
  line-height: 70rpx;
  text-align: center;
  margin: 0 20rpx;
}
.detail .detail_bottom .bottom_right {
  display: inline-block;
  width: 343rpx;
  height: 68rpx;
  border: 1rpx solid #75726a;
  color: #75726a;
  font-size: 24rpx;
  line-height: 70rpx;
  text-align: center;
}
