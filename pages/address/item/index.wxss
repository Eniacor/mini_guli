.address .address_list {
  background: #f9f9f9;
}
.address .address_list .list_item {
  background: #fff;
  padding: 20px 0 0 0;
  margin-bottom: 20px;
}
.address .address_list .list_item .item_top {
  padding: 0 15px;
}
.address .address_list .list_item .item_top .top_name {
  font-size: 16px;
  line-height: 16px;
  color: #333;
  margin-right: 15px;
}
.address .address_list .list_item .item_top .top_mobile {
  font-size: 16px;
  margin-right: 15px;
}
.address .address_list .list_item .item_top .top_default {
  display: inline-block;
  font-size: 12px;
  width: 42px;
  height: 20px;
  text-align: center;
  line-height: 20px;
  color: #F19736;
  border: 1px solid #F19736;
  border-radius: 6px;
}
.address .address_list .list_item .item_middle {
  padding: 15px 15px 20px 15px;
  font-size: 14px;
  line-height: 14px;
  color: #999;
}
.address .address_list .list_item .item_bottom {
  overflow: hidden;
  padding: 6px 15px 6px 15px;
  border-top: 1px solid #f2f2f2;
}
.address .address_list .list_item .item_bottom .bottom_left {
  float: left;
}
.address .address_list .list_item .item_bottom .bottom_left .van-checkbox .van-checkbox__icon-wrap .van-checkbox__icon {
  width: 14px;
  height: 13px;
}
.address .address_list .list_item .item_bottom .bottom_left .van-checkbox .van-checkbox__label {
  margin-left: 6px;
  font-size: 12px;
  line-height: 12px;
  color: #999;
}
.address .address_list .list_item .item_bottom .bottom_right {
  float: right;
}
.address .address_list .list_item .item_bottom .bottom_right .right_item {
  float: right;
  margin-right: 16px;
}
.address .address_list .list_item .item_bottom .bottom_right .right_item .item_txt {
  font-size: 12px;
  color: #999;
  margin-left: 6px;
}
.address .address_list .list_item .item_bottom .bottom_right .right_item:first-child {
  margin-right: 0;
}
.address .address_list .list_item:last-child {
  margin-bottom: 0;
}
.address .van-checkbox {
  margin: 25px 20px 6px 20px;
}
.address .van-checkbox .van-checkbox__icon-wrap .van-icon {
  width: 18px;
  height: 18px;
}
.address .van-checkbox .van-checkbox__label {
  font-size: 12px;
  color: 12px;
  color: #999;
}
.address .btn button {
  margin-left: 40rpx;
  margin-top: 40rpx;
  width: 670rpx;
  height: 86rpx;
  line-height: 86rpx;
  background: red;
  color: #fff;
  border-radius: 16rpx;
  outline: none;
}
