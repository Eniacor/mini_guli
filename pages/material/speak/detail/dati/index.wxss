.head {
  padding: 0 30rpx;
  overflow: hidden;
}
.head .head_left {
  float: left;
  margin-top: 40rpx;
  margin-right: 20rpx;
  width: 62rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  background: #face64;
  color: #fff;
  font-size: 28rpx;
  border-radius: 4rpx;
}
.head .head_middle {
  float: left;
  margin-top: 40rpx;
  font-size: 34rpx;
  line-height: 34rpx;
  font-weight: 600;
}
.head .head_right {
  float: right;
  margin-top: 30rpx;
  width: 150rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  color: #fff;
  border-radius: 30rpx;
  background: #d43c33;
  font-size: 28rpx;
}
.title {
  margin-top: 20rpx;
  padding: 0 30rpx;
  overflow: hidden;
}
.title .title_left {
  float: left;
  width: 46rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  margin-right: 30rpx;
  font-size: 22rpx;
  border-radius: 4rpx;
  color: #d43c33;
  border: 1rpx solid #d43c33;
}
.title .title_middle {
  float: left;
  width: 120rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  margin-right: 30rpx;
  font-size: 22rpx;
  border-radius: 4rpx;
  color: #d43c33;
  border: 1rpx solid #d43c33;
}
.title .star {
  float: left;
  height: 32rpx;
  line-height: 32rpx;
}
.wxParse {
  padding: 30rpx 30rpx 40rpx 30rpx;
  font-size: 32rpx;
  color: #222;
  line-height: 45rpx;
}
