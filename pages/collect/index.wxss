.collect .list .item {
  overflow: hidden;
  border: 1rpx solid #f2f2f2;
  padding: 26rpx 40rpx 39rpx 82rpx;
}
.collect .list .item .item_left {
  float: left;
}
.collect .list .item .item_left .left_img {
  width: 180rpx;
  height: 180rpx;
  background: #8E8E93;
}
.collect .list .item .item_right {
  float: left;
  width: 426rpx;
  margin-left: 12rpx;
  position: relative;
}
.collect .list .item .item_right .right_title {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #333333;
}
.collect .list .item .item_right .right_des {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #999999;
  margin-top: 20rpx;
  margin-bottom: 32rpx;
}
.collect .list .item .item_right .right_count {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #ff000d;
  margin-bottom: 26rpx;
}
.collect .list .item .item_right .right_price {
  font-size: 30rpx;
  line-height: 30rpx;
  color: #ff000d;
}
.collect .list .item .item_right .right_delete {
  position: absolute;
  bottom: 6rpx;
  right: 0;
  width: 34rpx;
  height: 36rpx;
}
.collect .empty {
  position: relative;
}
.collect .empty .empty_img {
  position: absolute;
  top: 322rpx;
  left: 51rpx;
  width: 648rpx;
  height: 468rpx;
}
.collect .empty .empty_txt {
  position: absolute;
  top: 796rpx;
  left: 0;
  width: 750rpx;
  text-align: center;
  font-size: 26rpx;
  color: #999999;
}
