.collect .collect_num {
  font-size: 24rpx;
  text-align: center;
  color: #999;
}
.collect .collect_can {
  padding: 0 36rpx;
  padding-top: 30rpx;
}
.collect .collect_can .item {
  display: inline-block;
  width: 318rpx;
  width: 46rpx;
  height: 46rpx;
  margin-right: 6rpx;
}
.collect .collect_can .item:last-child {
  margin-right: 0;
}
.collect .collect_can .item .img {
  width: 46rpx;
  height: 46rpx;
}
.collect .collect_btn {
  text-align: center;
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #999;
}
